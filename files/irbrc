begin
  require "pry"
  Pry.start
  exit
rescue LoadError => e
  warn "=> Unable to load pry"
end

def notify_roadhouse(message)
  Net::HTTP.post-form(
    URI('https://api.pushover.net/1/messages'),
    user: 'u9sh4ah5kdmwd5utk5vsszqgwpth1p', 
    token:'ajhkz4dqk5a3gnkckqi33cjmo7pwu6', 
    message: message
  )
end
